// ScalarOp:28
	float n_in28p1 = 0.50000;
	float n_out28p0 = n_out26p0 + n_in28p1;

// VectorCompose:30
	float n_in30p2 = 0.00000;
	vec3 n_out30p0 = vec3(n_out29p0, n_out28p0, n_in30p2);

